{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DEV",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "toolArgs": [
                "--dart-define", "ENV=dev",
            ],
        },
        {
            "name": "PROD",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define", "ENV=prod",
            ],
        },
        {
            "name": "TEST",
            "request": "launch",
            "type": "dart",
            "program": "./test/",
            "toolArgs": [
                "--dart-define", "ENV=prod",
                "--coverage"
            ],
        },
    ]
}